<!--pages/billing/billing.wxml-->
<!-- <text>pages/billing/billing.wxml</text> -->
<view class='container'>
    <view class='number'>
        <text>当前单车编号：{{num}}</text>
    </view>
    <view class='number-content'>
        <text>正在计费：</text>
        <view class='number'>{{hours}}:{{minutes}}:{{seconds}}</view>
    </view>
    <view class='endride'>
      <button type='warn' bindtap='endride' disabled='{{disabled}}'>结束骑行</button>
      <button type='primary' bindtap='moveToIndex'>回到地图</button>
    </view>
</view>
