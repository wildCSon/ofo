/* pages/billing/billing.wxss */
.container{
  width: 100%;
  height: 100vh;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}

.number,.endride{
  width: 100%;
  text-align: center;
  padding: 60rpx 0;
  flex: 2;
}

.number-content{
  flex:6;
  text-align: center;
  width: 100%;
}

.number-content view{
  font-size: 100rpx;
}

.endride button{
  width: 90%;
  margin-top:40rpx;
}